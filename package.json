{
  "name": "ubiquiti",
  "private": true,
  "scripts": {
    "find:unused": "next-unused",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier --write \"./**/*.{ts,tsx,json}\""
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@logger/*": [
        "logger/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@styles/*": [
        "stlyles/*"
      ]
    }
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "firebase": "^8.9.0",
    "lodash": "^4.17.21",
    "next": "12.0.7",
    "pino": "^7.6.1",
    "pino-pretty": "^7.3.0",
    "prettier": "^2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rich-markdown-editor": "^11.21.3",
    "theme-ui": "^0.12.1"
  },
  "devDependencies": {
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "next-unused": "0.0.6",
    "typescript": "4.5.4"
  }
}
